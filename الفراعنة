import discord
from discord.ext import commands

bot = commands.Bot(command_prefix='!')

@bot.event
async def on_ready():
    print(f'تم تسجيل الدخول كـ {bot.user}')

@bot.command()
async def مرحبا(ctx):
    await ctx.send('أهلاً وسهلاً بك!')

